from car import Car

def save_Car(file, car_list):
    with open(file, 'w') as f:
        for car in car_list:
            f.write(str(car) + '\n')

def upload_car(file):
    cars = []
    try:
        with open(file, 'r') as f:
            for line in f:
                car = Car.from_string(line)
                cars.append(car)
        print(f'Cars uploaded de {file} successfully.')
    except FileNotFoundError:
        print (f'File {file} was not found.')
    return cars
